{
    "name": "padosoft/laravel-request",
    "description": "Laravel Package to interact with Illuminate Request and Files Upload.",
    "keywords": [
        "padosoft",
        "laravel-request",
        "request",
        "upload"
    ],
    "homepage": "https://github.com/padosoft/laravel-request",
    "license": "MIT",
    "authors": [
        {
            "name": "Lorenzo Padovani",
            "email": "helpdesk@padosoft.com",
            "homepage": "https://www.padosoft.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0.0",
        "illuminate/support": "^5.0",
        "illuminate/http": "^5.0",
        "padosoft/io": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.2",
        "phpunit/phpunit": "^5.0",
        "mockery/mockery": "^0.9.5",
        "padosoft/test": "~0.2"
    },
    "autoload": {
        "psr-4": {
            "Padosoft\\Laravel\\Request\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Padosoft\\Laravel\\Request\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
